{
  "name": "NOZIP",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Add no-zip to subject",
  "homepage_url": "https://github.com/kuwa72/chrome-extension-no-zip",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
          "scripts": ["background.js"],
          "persistent": false
  }
}
